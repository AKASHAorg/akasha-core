{
  "name": "@akashaorg/composedb-models",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "composedb models used by AKASHA",
  "author": "AKASHA Foundation <hello@akasha.org> (https://akasha.org/)",
  "contributors": [
    "Marius Darila <marius.darila@akasha.world> (https://github.com/kenshyx)",
    "Sever Abibula <sever.abibula@akasha.world> (https://github.com/SeverS)"
  ],
  "homepage": "https://github.com/AKASHAorg/akasha-framework#readme",
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AKASHAorg/akasha-framework.git"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "tsc": "tsc --build",
    "clean": "tsc --build --clean && rimraf lib dist node_modules tsconfig.tsbuildinfo",
    "create": "node ./src/create-composite.mjs",
    "merge": "node ./src/merge-composites.mjs",
    "compile": "composedb composite:compile build/merged-composite.json lib/runtime-composite.json --ceramic-url=https://staging-ceramic.akasha.world",
    "start:graphql": "composedb graphql:server --ceramic-url=https://staging-ceramic.akasha.world --graphiql lib/runtime-composite.json --port=5005"
  },
  "bugs": {
    "url": "https://github.com/AKASHAorg/akasha-framework/issues"
  },
  "devDependencies": {
    "@ceramicnetwork/http-client": "^2.23.0",
    "@composedb/cli": "^0.4.5",
    "@composedb/devtools": "^0.4.4",
    "@composedb/devtools-node": "^0.4.4",
    "dids": "^4.0.3",
    "dotenv": "16.3.1",
    "key-did-provider-ed25519": "^3.0.1",
    "key-did-resolver": "^3.0.0",
    "uint8arrays": "^4.0.4"
  }
}
