  "scripts": {
    "clean": "tsc --build --clean tsconfig.json",
    "clean:all": "yarn clean && nx reset",
    "prepare": "nx run-many --target=prepare --all --verbose",
    "build:libs": "nx run-many -t build --projects=tag:type:lib --verbose",
    "build:apps": "nx run-many -t build --projects=tag:scope:extension --verbose",
    "extract:translations": "nx run-many -t translations --projects=tag:scope:extension --verbose",
    "build:all": "yarn && yarn prepare && yarn build:libs && yarn build:apps && yarn extract:translations",
    "apps:watch": "nx watch --projects=tag:scope:extension --includeDependentProjects -- nx build \\$NX_PROJECT_NAME",
    "world:dev": "nx start @akashaorg/akasha.world --verbose",
    "world:deploy": "nx deploy @akashaorg/akasha.world --verbose",
    "extensions:upload": "nx run @akashaorg/core-sdk:prepare-local && NODE_OPTIONS=\"--no-warnings\" tsx ./tools/scripts/upload-w3-storage.mjs"
  },
