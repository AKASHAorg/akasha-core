{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-hooks"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "interface-name": false,
    "only-arrow-functions": false,
    "prettier": true,
    "react-hooks-nesting": "error",
    "import-name": false,
    "jsx-no-lambda": false,
    "function-name": [ true, { "method-regex": "^[a-z][\\w\\d]+$", "private-method-regex": "^[a-z_][\\w\\d]+$", "protected-method-regex": "^[a-z_][\\w\\d]+$", "static-method-regex": "^[A-Z_\\d]+$", "function-regex": "^[a-z][\\w\\d]+$" } ],
    "no-increment-decrement": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
