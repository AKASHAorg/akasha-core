{
  "name": "@akashaproject/app-3box-integration",
  "private": true,
  "version": "0.1.0",
  "description": "3box integration package",
  "author": "AKASHA Foundation <hello@akasha.org> (https://akasha.org/)",
  "contributors": [
    "Marius Darila <marius.darila@akasha.world> (https://github.com/kenshyx)",
    "Sever Abibula <sever.abibula@akasha.world> (https://github.com/SeverS)"
  ],
  "homepage": "https://github.com/AkashaProject/akasha-world-framework#readme",
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AKASHAorg/akasha-world-framework.git"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc --build",
    "tsc": "tsc --build",
    "extract:translations": "i18next --config ./i18next-parser.config.js",
    "prepare": "npm run tsc",
    "pack": "NODE_OPTIONS='--max-old-space-size=4096' webpack --config webpack.config.js",
    "postpack": "copyfiles -f ./dist/* ../../ui/build/apps/3box",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "lib/*",
      "dist/*",
      "build/*"
    ],
    "exec": "npm run pack || exit 1",
    "ext": "ts,tsx"
  },
  "bugs": {
    "url": "https://github.com/AKASHAorg/akasha-world-framework/issues"
  },
  "dependencies": {
    "@akashaproject/sdk-core": "*",
    "@akashaproject/sdk-common": "*",
    "@akashaproject/sdk-runtime": "*",
    "@akashaproject/sdk-db": "*",
    "@akashaproject/design-system": "*",
    "3box": "1.22.2",
    "react-i18next": "11.7.3",
    "react-router-dom": "5.2.0",
    "single-spa-react": "3.2.0",
    "easy-peasy": "3.3.1",
    "path-browserify": "1.0.1",
    "util": "0.12.3",
    "assert": "2.0.0",
    "crypto-browserify": "3.12.0",
    "stream-browserify": "3.0.0"
  },
  "devDependencies": {
    "@akashaproject/ui-awf-typings": "*",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-router-dom": "5.1.6",
    "@types/react-test-renderer": "16.9.3",
    "@types/single-spa-react": "2.12.0",
    "nodemon": "2.0.6"
  },
  "peerDependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rxjs": "6.6.3"
  }
}
