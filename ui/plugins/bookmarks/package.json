{
  "name": "@akashaproject/ui-plugin-bookmarks",
  "version": "0.1.0",
  "description": "Plugin to show bookmarked content",
  "author": "AKASHA Foundation <hello@akasha.org> (https://akasha.org/)",
  "contributors": [
    "Sever Abibula <sever.abibula@akasha.world> (https://github.com/SeverS)",
    "Marius Darila <marius.darila@akasha.world> (https://github.com/kenshyx)",
    "Valentin Cotea <valentin.cotea@akasha.world> (https://github.com/quininez)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AKASHAorg/akasha-world-framework.git"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "tsc --build",
    "tsc": "tsc --build",
    "extract:translations": "i18next --config ./i18next-parser.config.js",
    "prepare": "npm run tsc",
    "pack": "NODE_OPTIONS='--max-old-space-size=4096' webpack --config webpack.config.js",
    "postpack": "copyfiles -f ./dist/* ../../build/plugins/bookmarks",
    "start": "webpack-dev-server --config ./webpack.config.dev.js --port 8333",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "lib/*",
      "dist/*",
      "build/*"
    ],
    "exec": "npm run pack || exit 1",
    "ext": "ts,tsx"
  },
  "dependencies": {
    "@akashaproject/ui-awf-typings": "*",
    "@akashaproject/design-system": "*",
    "@akashaproject/sdk-db": "*",
    "@akashaproject/sdk-auth": "*",
    "@akashaproject/sdk-common": "*",
    "@akashaproject/sdk-profiles": "*",
    "@akashaproject/ui-awf-hooks": "*",
    "@akashaproject/sdk-posts": "*",
    "@akashaproject/ui-widget-trending": "*",
    "@types/i18next": "13.0.0",
    "@types/react-i18next": "8.1.0",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.6",
    "react-i18next": "11.3.1",
    "react-router-dom": "5.2.0",
    "single-spa-react": "3.2.0"
  },
  "devDependencies": {
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-test-renderer": "16.9.3",
    "@types/single-spa-react": "2.12.0",
    "acorn-jsx": "5.3.1",
    "jest-styled-components": "7.0.3",
    "react-test-renderer": "16.14.0"
  },
  "peerDependencies": {
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "files": [
    "lib"
  ]
}
