{
  "name": "@akashaproject/ui-plugin-notifications",
  "version": "0.1.0",
  "description": "Plugin for / url handler",
  "author": "AKASHA Foundation <hello@akasha.org> (https://akasha.org/)",
  "contributors": [
    "Sever Abibula <sever.abibula@akasha.world> (https://github.com/SeverS)",
    "Marius Darila <marius.darila@akasha.world> (https://github.com/kenshyx)",
    "Valentin Cotea <valentin.cotea@akasha.world> (https://github.com/quininez)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkashaProject/akasha-sdk-js.git"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "tsc --build",
    "tsc": "tsc --build",
    "extract:translations": "i18next --config ./i18next-parser.config.js",
    "prepare": "npm run tsc",
    "pack": "NODE_OPTIONS='--max-old-space-size=4096' webpack --config webpack.config.js",
    "postpack": "copyfiles -f ./dist/* ../../build/plugins/notifications",
    "start": "webpack-dev-server --config ./webpack.config.dev.js --port 8333",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "lib/*",
      "dist/*",
      "build/*"
    ],
    "exec": "npm run pack || exit 1",
    "ext": "ts,tsx"
  },
  "dependencies": {
    "@akashaproject/ui-awf-hooks": "*",
    "@akashaproject/design-system": "*",
    "@akashaproject/sdk-auth": "*",
    "@akashaproject/sdk-posts": "*",
    "@akashaproject/sdk-profiles": "*",
    "@akashaproject/sdk-common": "*",
    "@types/i18next": "13.0.0",
    "@types/react-i18next": "8.1.0",
    "@types/react-router": "5.1.13",
    "@types/react-router-dom": "5.1.7",
    "react-i18next": "11.8.13",
    "react-router-dom": "5.2.0",
    "single-spa-react": "4.1.1"
  },
  "devDependencies": {
    "@akashaproject/ui-awf-typings": "*",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-test-renderer": "17.0.1",
    "@types/single-spa-react": "3.0.1"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "files": [
    "lib"
  ]
}
