{
  "name": "@akashaproject/ui-plugin-search",
  "version": "0.1.0",
  "description": "Plugin for / url handler",
  "author": "AKASHA Foundation <hello@akasha.org> (https://akasha.org/)",
  "contributors": [
    "Sever Abibula <sever.abibula@akasha.world> (https://github.com/SeverS)",
    "Marius Darila <marius.darila@akasha.world> (https://github.com/kenshyx)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkashaProject/akasha-sdk-js.git"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "tsc --build",
    "tsc": "tsc --build",
    "extract:translations": "i18next --config ./i18next-parser.config.js",
    "prepare": "npm run tsc",
    "pack": "NODE_OPTIONS='--max-old-space-size=4096' webpack --config webpack.config.js",
    "postpack": "copyfiles -f ./dist/*search.* ../../build/plugins",
    "start": "webpack-dev-server --config ./webpack.config.dev.js --port 8333",
    "pack:watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "lib/*",
      "dist/*",
      "build/*"
    ],
    "exec": "npm run pack",
    "ext": "ts,tsx"
  },
  "dependencies": {
    "@akashaproject/ui-awf-typings": "*",
    "@akashaproject/design-system": "*",
    "@types/i18next": "^13.0.0",
    "@types/react-i18next": "^8.1.0",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "immer": "^5.3.2",
    "react-i18next": "^11.3.1",
    "react-router-dom": "^5.1.2",
    "react-tracked": "1.2.0",
    "single-spa-react": "^2.11.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/single-spa-react": "^2.8.3",
    "acorn-jsx": "5.x",
    "jest-styled-components": "^7.0.0",
    "react-test-renderer": "^16.12.0"
  },
  "peerDependencies": {
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "files": [
    "lib"
  ]
}
