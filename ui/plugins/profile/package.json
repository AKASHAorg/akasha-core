{
  "name": "@akashaproject/ui-plugin-profile",
  "version": "0.1.0",
  "description": "Plugin for /profile url handler",
  "author": "AKASHA Foundation <hello@akasha.org> (https://akasha.org/)",
  "contributors": [
    "Sever Abibula <sever.abibula@akasha.world> (https://github.com/SeverS)",
    "Marius Darila <marius.darila@akasha.world> (https://github.com/kenshyx)",
    "Valentin Cotea <valentin.cotea@akasha.world> (https://github.com/quininez)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkashaProject/akasha-sdk-js.git"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "tsc --build",
    "tsc": "tsc --build",
    "extract:translations": "i18next --config ./i18next-parser.config.js",
    "prepare": "npm run tsc",
    "pack": "NODE_OPTIONS='--max-old-space-size=4096' webpack --config webpack.config.js",
    "postpack": "copyfiles -f ./dist/* ../../build/plugins/profile",
    "start": "webpack-dev-server --config ./webpack.config.js --port 8332",
    "watch": "webpack --config ./webpack.config.js --watch",
    "pretest": "tsc -m commonjs",
    "test": "jest",
    "clean": "tsc --build --clean"
  },
  "dependencies": {
    "@akashaproject/design-system": "*",
    "@akashaproject/awf-sdk": "*",
    "@akashaproject/ui-awf-hooks": "*",
    "@akashaproject/ui-widget-feed": "*",
    "@types/react-router": "5.1.16",
    "@types/react-router-dom": "5.1.8",
    "react-i18next": "11.11.4",
    "react-router-dom": "5.2.0",
    "single-spa-react": "4.3.1",
    "i18next": "20.4.0",
    "i18next-browser-languagedetector": "6.1.2",
    "i18next-fetch-backend": "3.0.0",
    "i18next-chained-backend": "3.0.2",
    "i18next-localstorage-backend": "3.1.3",
    "react-query": "3.19.6"
  },
  "devDependencies": {
    "@akashaproject/ui-awf-typings": "*",
    "@akashaproject/ui-awf-testing-utils": "*",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "@types/react-test-renderer": "17.0.1"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "files": [
    "lib"
  ]
}
