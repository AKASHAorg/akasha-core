{
  "name": "@akashaproject/ui-plugin-profile",
  "version": "0.1.0",
  "description": "Plugin for /profile url handler",
  "author": "AKASHA Foundation <hello@akasha.org> (https://akasha.org/)",
  "contributors": [
    "Sever Abibula <sever.abibula@akasha.world> (https://github.com/SeverS)",
    "Marius Darila <marius.darila@akasha.world> (https://github.com/kenshyx)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkashaProject/akasha-sdk-js.git"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "tsc --build",
    "test": "jest",
    "tsc": "tsc --build",
    "extract:translations": "npx i18next src/components/**/*.tsx -r -c ./i18next-parser.config.js",
    "prepare": "npm run tsc",
    "pack": "NODE_OPTIONS='--max-old-space-size=4096' webpack --config webpack.config.js",
    "postpack": "copyfiles -f ./dist/*profile.* ../../build/plugins",
    "start": "webpack-dev-server --config ./webpack.config.dev.js --port 8332"
  },
  "dependencies": {
    "@akashaproject/design-system": "*",
    "@types/i18next": "^13.0.0",
    "@types/react-i18next": "^8.1.0",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.1",
    "i18next-fetch-backend": "^2.2.0",
    "immer": "^5.0.0",
    "react-i18next": "^11.0.1",
    "react-router-dom": "^5.1.2",
    "react-tracked": "^0.11.0",
    "single-spa-react": "^2.10.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-test-renderer": "^16.9.1",
    "@types/single-spa-react": "^2.8.3",
    "acorn-jsx": "5.x",
    "i18next-parser": "^1.0.0-beta34",
    "jest-styled-components": "^6.3.3",
    "react-test-renderer": "^16.11.0"
  },
  "peerDependencies": {
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "files": [
    "lib"
  ]
}
